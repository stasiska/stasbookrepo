{
    "compilerOptions": {
      
      "module": "CommonJS",
      "outDir": "dist",
      "rootDir": "src",
      "declaration": true,
      "declarationMap": true,
      "emitDeclarationOnly": false,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "moduleResolution": "node",
      "paths": {
        "@lib/grpc/*": ["./src/*"]
      },
      "target": "ES2022",
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
    },
    "include": ["src"]
  }